                    <!-- ============================================================== -->
                    <!-- Email campaign chart -->
                    <!-- ============================================================== -->
                    <!-- ============================================================== -->
                    <!-- Ravenue - page-view-bounce rate -->
                    <!-- ============================================================== -->
                    <div class="row">
                        <!-- column -->
                        <div class="col-12">
                            <div class="card">
                                <div class="card-body">
                                    <h4 class="card-title">Latest Sales</h4>
                                </div>
                                <div class="table-responsive">
                                    <table class="table table-hover">
                                        <thead>
                                            <tr>
                                                <th class="border-top-0">ORDER ID</th>
                                                <th class="border-top-0">STATUS</th>
                                                <th class="border-top-0">DATE</th>
                                                <th class="border-top-0">PRICE</th>
                                            </tr>
                                        </thead>
                                        <tbody>
                                            <% latestSales.forEach((sales)=>{%>
                                                <tr>
                                                    <td class="txt-oflo">
                                                        <%=sales._id%>
                                                    </td>
                                                    <% if(sales.isCompleted){%>
                                                        <td><span
                                                                class="label label-success label-rounded">SUCCESS</span>
                                                        </td>
                                                        <% }else{%>
                                                            <td><span
                                                                    class="label label-warning label-rounded">PENDING</span>
                                                            </td>
                                                            <% } %>
                                                                <td class="txt-oflo">
                                                                    <%=sales.createdAt.toLocaleDateString();%>
                                                                </td>
                                                                <td><span class="font-medium">₹ <%=sales.bill%>
                                                                            .00</span></td>
                                                </tr>
                                                <% }) %>
                                        </tbody>
                                    </table>
                                </div>
                            </div>
                        </div>
                    </div>
                    <!-- ============================================================== -->
                    <!-- Ravenue - page-view-bounce rate -->
                    <!-- ============================================================== -->
                    <!-- ============================================================== -->
                    <!-- Recent comment and chats -->
                    <!-- ============================================================== -->

                    <!-- ============================================================== -->
                    <!-- Recent comment and chats -->

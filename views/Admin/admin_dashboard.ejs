    <script src="https://cdn.jsdelivr.net/npm/chart.js"></script>
    <script>
        const packed = document.getElementById('packed').value
        const shipped = document.getElementById('shipped').value
        const cancelled = document.getElementById('cancelled').value
        const ordered = document.getElementById('ordered').value
        const delivered = document.getElementById('delivered').value

        const labels = [
            'packed',
            'shipped',
            'ordered',
            'delivered',
            'cancelled',
        ];

        const data = {
            labels: labels,
            datasets: [{
                label: 'Sales in Rs',
                backgroundColor: [
                    'rgb(175,225,80)',
                    'rgb(54, 162, 235)',
                    'rgb(255, 205, 86)',
                    'rgb(175,225,175)',
                    'rgb(255, 99, 132)'
                ],
                data: [packed,
                    shipped,
                    ordered,
                    delivered,
                    cancelled]
            }]
        };
        const config = {
            type: 'doughnut',
            data: data,
            options: {}
        };
        const orderStatusDoughnut = new Chart(
            document.getElementById('orderStatusDoughnut'),
            config
        );
    </script>
    <script>
        let arr = [];
        console.log(document.getElementById("array").value);
        arr = document.getElementById("array").value.split(",");
        console.log(typeof (arr[0]))
        let b = arr.map(function (item) {
            return parseInt(item, 10);
        });
        const labels1 = ['Day1', 'Day2', 'Day3', 'Day4', 'Day5',]
        const data1 = {
            labels: labels1,
            datasets: [{
                label: 'Sales',
                // data: [65, 59, 80, 81, 56, 55, 40],
                data: [...b],
                fill: false,
                borderColor: 'rgb(75, 192, 192)',
                tension: 0.1
            }]
        };
        const config1 = {
            type: 'line',
            data: data1,
        };
        const orderStatusDoughnut1 = new Chart(
            document.getElementById('orderStatusDoughnut1'),
            config1
        );
    </script>
    <script>
        function downloadPDF() {
            const canvas = document.getElementById('orderStatusDoughnut1')
            const canvasImage = canvas.toDataURL('image/jpeg', 1.0)

            let pdf = new jsPDF('landscape')
            pdf.setFontSize(20)
            pdf.addImage(canvasImage, 'JPEG', 15, 15, 265, 150)
            pdf.save('salesrecord.pdf')
        }
        function downloadSecPDF() {
            const canvas = document.getElementById('orderStatusDoughnut')
            const canvasImage = canvas.toDataURL('image/jpeg', 1.0)

            let pdf = new jsPDF('landscape')
            pdf.setFontSize(20)
            pdf.addImage(canvasImage, 'JPEG', 15, 15, 265, 150)
            pdf.save('orderrecord.pdf')
        }
    </script>

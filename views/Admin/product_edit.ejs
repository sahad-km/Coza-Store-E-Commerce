        <!-- Page wrapper  -->
        <!-- ============================================================== -->
        <div class="page-wrapper">
            <!-- ============================================================== -->
            <!-- Bread crumb and right sidebar toggle -->
            <!-- ============================================================== -->
            <div class="page-breadcrumb">
                <div class="row">
                    <div class="col-5 align-self-center">
                        <h4 class="page-title">Edit Product</h4>
                    </div>
                    <div class="col-7 align-self-center">
                        <div class="d-flex align-items-center justify-content-end">
                            <nav aria-label="breadcrumb">
                                <ol class="breadcrumb">
                                    <li class="breadcrumb-item">
                                        <a href="#">Home</a>
                                    </li>
                                    <li class="breadcrumb-item active" aria-current="page">Products</li>
                                    <li class="breadcrumb-item active" aria-current="page">Edit Product</li>
                                </ol>
                            </nav>
                        </div>
                    </div>
                </div>
            </div>
            
            <div class="container-fluid">
              <div class="row">
                <div class="col-md-12">
                  <form class="form-horizontal" action="/admin/updateproduct/<%=editProduct._id%>?_method=PUT" method="POST" enctype="multipart/form-data">
                    <fieldset>
                  
                      <!-- Form Name -->
                      <legend>PRODUCTS</legend>
                  
                      <!-- Text input-->
                      <div class="form-group">
                        <label class="col-md-4 control-label" for="product_name">PRODUCT NAME</label>
                        <div class="col-md-4">
                          <input id="product_name" value="<%=editProduct.product_name%> " name="product_name" placeholder="PRODUCT NAME" class="form-control input-md"
                            required="" type="text">
                        </div>
                      </div>
                  
                      <!-- Select Basic -->
                      <div class="form-group">
                        <label class="col-md-4 control-label" for="product_brand">PRODUCT BRAND</label>
                        <div class="col-md-4">
                          <select id="product_brand" name="product_brand" class="form-control">
                              <option selected value="<%=editProduct.product_brand%>"><%=editProduct.product_brand%></option>
                            <% for(values of brandDisplay ){ %>
                              <option value=" <%= values.brand_name %>">
                                <%= values.brand_name %>
                              </option>
                              <% } %>
                          </select>
                        </div>
                      </div>
                  
                      <!-- Textarea -->
                      <div class="form-group">
                        <label class="col-md-4 control-label" for="product_description">PRODUCT DESCRIPTION</label>
                        <div class="col-md-4">
                          <textarea class="form-control" id="product_description" name="product_description"><%=editProduct.product_description%></textarea>
                        </div>
                      </div>
                  
                      <div class="form-group">
                        <label class="col-md-4 control-label" for="">4 GB 64 GB</label>
                        <div class="col-md-4">
                          <input  name="stock1" placeholder="NO.OF STOCKS" value="<%=editProduct.product_stock[0].stock1%>" class="form-control input-md"
                            required type="number">
                        </div>
                      </div>
                  
                      <div class="form-group">
                        <label class="col-md-4 control-label" for="">4 GB 128 GB</label>
                        <div class="col-md-4">
                          <input name="stock2" placeholder="NO.OF STOCKS" value="<%=editProduct.product_stock[0].stock2%>" class="form-control input-md"
                            required type="number">
                        </div>
                      </div>
                  
                      <!-- Textarea -->
                      <div class="form-group">
                        <label class="col-md-4 control-label" for="">6 GB 128 GB</label>
                        <div class="col-md-4">
                          <input name="stock3" placeholder="NO.OF STOCKS" value="<%=editProduct.product_stock[0].stock3%>" class="form-control input-md"
                            required type="number">
                        </div>
                      </div>
                  
                      <div class="form-group">
                        <label class="col-md-4 control-label" for="">8 GB 256 GB</label>
                        <div class="col-md-4">
                          <input name="stock4" placeholder="NO.OF STOCKS" value="<%=editProduct.product_stock[0].stock4%>" class="form-control input-md"
                            required type="number">
                        </div>
                      </div>
                  
                      <div class="form-group">
                        <label class="col-md-4 control-label" for="">12 GB 1 TB</label>
                        <div class="col-md-4">
                          <input  name="stock5" placeholder="NO.OF STOCKS" value="<%=editProduct.product_stock[0].stock5%>" class="form-control input-md"
                            required type="number">
                        </div>
                      </div>
                      <!-- Text input-->
                      <div class="form-group">
                        <label class="col-md-4 control-label" for="selling_price">SELLING PRICE</label>
                        <div class="col-md-4">
                          <input id="selling_price" name="selling_price" placeholder="SELLING PRICE" value="<%=editProduct.selling_price%>" class="form-control input-md"
                            required type="number">
                  
                        </div>
                      </div>
                  
                      <!-- Text input-->
                      <div class="form-group">
                        <label class="col-md-4 control-label" for="org_price">MRP</label>
                        <div class="col-md-4">
                          <input id="org_price" name="org_price" placeholder="MRP" value="<%=editProduct.org_price%>" class="form-control input-md" required="" type="text">
                        </div>
                      </div>
                  
                  
                      <!-- File Button -->
                      <div class="form-group">
                        <label class="col-md-4 control-label" for="image">PRODUCT IMAGE</label>
                        <div class="col-md-4">
                          <input name="image" class="input-file" value="<%=editProduct.image[0].url%> " type="file" multiple>
                        </div>
                      </div>
                  
                      <!-- Button -->
                      <div class="form-group">
                        <label class="col-md-4 control-label" for="singlebutton">INSERT PRODUCT</label>
                        <div class="col-md-4">
                          <button type="submit" id="singlebutton" name="singlebutton" class="btn btn-primary">Add Product</button>
                        </div>
                      </div>
                    </fieldset>
                  </form>
            </div>
            </div>
